@model IEnumerable<WebDeveloper.Model.AddressType>
@using WebDeveloper.Helpers
@{
    ViewBag.Title = "List AddressType";
}

<h2>List AddressType</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.rowguid)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ModifiedDate)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.rowguid)
        </td>
        <td>
            @Html.DisplayDateOrNullExtension(item.ModifiedDate)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.AddressTypeID }) |
            @Html.ActionLink("Details", "Details", new { id=item.AddressTypeID }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.AddressTypeID })
        </td>
    </tr>
}

</table>
